{
  "name": "ztv",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "start": "concurrently --kill-others \"npm:peer:start\" \"node scripts/start.js\"",
    "ui:dev": "yarn workspace @ztv/ui dev",
    "ui:build": "yarn workspace @ztv/ui build",
    "server:build": "yarn workspace @ztv/server build",
    "server:dev": "yarn workspace @ztv/server dev",
    "peer:build": "yarn workspace @ztv/peer build",
    "peer:dev": "yarn workspace @ztv/peer dev",
    "peer:start": "yarn workspace @ztv/peer start"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
